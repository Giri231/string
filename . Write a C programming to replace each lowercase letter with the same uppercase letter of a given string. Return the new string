#include<iostream>

using namespace std;

string lowercase(string s,int n)
{
    for(int i=0;i<n;i++)
    {
        if(s[i]>='a' && s[i]<='z' )
        {
            s[i]=s[i]-32;
        }
    }
    return s;
}

int main()
{
    string s;
    getline(cin,s);
    int n=s.length();
   cout<< lowercase(s,n);
    return 0;
}
