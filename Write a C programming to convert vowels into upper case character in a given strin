#include<iostream>

using namespace std;

int main()
{
    string s;
    
    getline(cin,s);
    int n=s.length();
    int i,j;
    for( i=0;i<n;i++)
    {
        if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u')
        {
            s[i]=s[i]-32;
        }
    }
    cout<<s;
    return 0;
    
}
